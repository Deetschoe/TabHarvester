{
    "manifest_version": 3,
    "name": "Tab Harvestor",
    "description": "Copy all open tab URLs into a single list",
    "version": "1.0",
    "permissions": ["tabs", "clipboardWrite"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "ios/16.png",
            "48": "ios/50.png",
            "128": "ios/128.png"
        }
    },
    "icons": {
        "16": "ios/16.png",
        "48": "ios/50.png",
        "128": "ios/128.png"
    }
}
